{
	"name": "frames.flashsoft.eu",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "bun run echo-tunnel && vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"echo-tunnel": "bun run ./src/lib/cli/echo-tunnel.ts",
