		"@skeletonlabs/skeleton": "^3.1.2",
		"@skeletonlabs/skeleton-svelte": "^1.2.0",
		"@wagmi/core": "^2.16.7",
		"sass": "^1.86.3",
		"svelte-gestures": "^5.1.3",
		"viem": "^2.26.2",
		"wagmi": "^2.14.16"
